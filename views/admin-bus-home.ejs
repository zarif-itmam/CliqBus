<%- include('./layout/navbar-admin') %>

<table class="table table-striped table-bordered table-hover table-sm table-dark">
    <tr>
        <th>Bus Company Name</th>
        <th>Bus Type</th>
        <th>Seat Count</th>
        <th>Bus Rating</th>
    </tr>
    <% for (let i = 0; i < result.length; i++) { %>
        <tr>
            <td><%= result[i].BUS_COMPANY_NAME %></td>
            <td><%= result[i].BUS_TYPE %></td>
            <td><%= result[i].SEAT_COUNT %></td>
            <td><%= result[i].BUS_RATING %></td>
        </tr>
    <% } %>
</table>

<%- include('./layout/footer') %>

</body>
</html>

